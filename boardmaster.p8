__lua__
#include lib/mouse.lua
#include lib/animator.lua
#include lib/objects.lua
#include lib/player.lua
#include lib/tools.lua
#include lib/director.lua
#include src/main.lua
#include src/board.lua
#include src/results.lua
current_level = "main"

cur_time = 0

function changeLevel(level)
    current_level = level
    levels[current_level]:init()
end

function _init()
    cur_time = time()
    printh("Start")
    levels = { main = MainMenu, board = Board, results = ResultsMenu }
    -- changeLevel("board")
end

function _update60()
    cur_time = time()

    levels[current_level]:update()
end

function _draw()
    cls()
    levels[current_level]:draw()
end

